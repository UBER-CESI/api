{
    "check-coverage": true,
    "all": true,
    "include": [
        "src/**"
    ],
    "exclude": [
    ],
    "branches": 85,
    "lines": 85,
    "functions": 90,
    "statements": 85,
    "reporter": ["html"],
    "report-dir": "./coverage",
    "watermarks": {
        "lines": [80, 90],
        "functions": [50, 65],
        "branches": [64, 80],
        "statements": [80, 95]
      }
}
